<settings
        xmlns="http://maven.apache.org/SETTINGS/1.0.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 https://maven.apache.org/xsd/settings-1.0.0.xsd">
    <proxies>
        <proxy>
            <id>Rental-Car-2</id>
            <active>true</active>
            <protocol>http</protocol>
            <host>rental-car-2.com</host>
            <port>8080</port>
            <username>Pasha</username>
            <password>admin</password>
            <nonProxyHosts>*.rental-car-2.com|*.apache.org</nonProxyHosts>
        </proxy>
    </proxies>
    <mirrors>
        <mirror>
            <id>Rental-Car-2</id>
            <name>Enterprise</name>
            <url>https://rental-car-2.com/</url>
            <mirrorOf>*</mirrorOf>
        </mirror>
    </mirrors>
</settings>